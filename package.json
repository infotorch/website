{
  "name": "@infotorch/website",
  "version": "0.4.24",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@nivo/bar": "^0.61.1",
    "@nivo/core": "^0.61.0",
    "@nivo/line": "^0.61.1",
    "@sentry/browser": "^5.14.2",
    "autoprefixer": "^9.7.5",
    "axios": "^0.19.2",
    "axios-cache-adapter": "^2.5.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.10.0",
    "lodash.camelcase": "^4.3.0",
    "material-table": "^1.57.2",
    "numeral": "^2.0.6",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "query-string": "^6.11.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-ga": "^2.7.0",
    "react-hook-form": "^5.1.0",
    "react-icons": "^3.9.0",
    "react-image": "^2.4.0",
    "react-map-gl": "^5.2.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-table": "^7.0.0",
    "tailwindcss": "^1.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:css": "postcss src/styles/index.css -o src/styles/tailwind.css -w",
    "build:css": "postcss src/styles/index.css -o src/styles/tailwind.css --env production",
    "build": "react-scripts build",
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "eject": "react-scripts eject",
    "purgecf": "./cloudflare-purge.sh",
    "predeploy": "react-scripts build",
    "deploy": "aws s3 sync build s3://infotorch.org/ --acl public-read",
    "postdeploy": "yarn run purgecf",
    "postversion": "git push --tags"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "purge-cloudflare-cache": "^1.2.2",
    "source-map-explorer": "^2.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
